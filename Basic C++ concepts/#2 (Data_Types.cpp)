#include<iostream>
using namespace std;

int main() {

    //Data Types in C++.

    char Grade = 'S'; //Can take upto only 1 charecter.
    string Name = "T Mohamed Yaser"; // Can take multiple charecter.
    int Age = 18; //Can take a whole number without decimals.
    float points = 10; //Can take decimals upto 6 to 7 significant digits.
    double total_score = 8.97; //Can take decimals upto 15 to 16 significant digits.

    cout<< Grade <<endl;
    cout<< Name <<endl;
    cout<< Age <<endl;
    cout<< points <<endl;
    cout<< total_score <<endl;
    
    return 0;
}
